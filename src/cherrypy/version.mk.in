NAME		= opt-lifemapper-cherrypy
VERSION     = 18.6.0
ARCHIVENAME = CherryPy
RELEASE		= 1
WHEEL_POSTFIX	= whl

# cherrypy dependencies
PYTZ        = pytz-2020.1-py2.py3-none-any.$(WHEEL_POSTFIX)
JFUNCTOOLS  = jaraco.functools-3.0.1-py3-none-any.$(WHEEL_POSTFIX)
TEMPORA     = tempora-4.0.0-py3-none-any.$(WHEEL_POSTFIX)
PORTEND     = portend-2.6-py2.py3-none-any.$(WHEEL_POSTFIX)
JCLASSES    = jaraco.classes-3.1.0-py2.py3-none-any.$(WHEEL_POSTFIX)
