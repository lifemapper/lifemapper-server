JTEXT       = jaraco.text-3.2.0-py2.py3-none-any.$(WHEEL_POSTFIX)
JCOLLECTIONS= jaraco.collections-3.0.0-py2.py3-none-any.$(WHEEL_POSTFIX)
CHEROOT     = cheroot-8.4.5-py2.py3-none-any.$(WHEEL_POSTFIX)
ZC          = zc.lockfile-2.0-py2.py3-none-any.$(WHEEL_POSTFIX)
