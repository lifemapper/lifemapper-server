#!/bin/bash

# Purpose: Fetch @ARCHIVENAME@ wheel
DL_URL=https://files.pythonhosted.org/packages/dc/29/162476fd44203116e7980cfbd9352eef9db37c49445d1fec35509022f6aa
DL_WHL=@ARCHIVENAME@-@VERSION@-cp36-cp36m-manylinux1_x86_64.whl

get_wheel () {
  echo "Fetch @ARCHIVENAME@ wheel file"
  wget "$DL_URL/$DL_WHL"
}

### main ###
get_wheel
