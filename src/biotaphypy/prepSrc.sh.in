#!/bin/bash

# Purpose: download release of lmpy, lm python objects, from Github repository 

URL=https://github.com/biotaphy/@ARCHIVENAME@/releases/download/v@VERSION@
FNAME=biotaphy-@VERSION@-py3-none-any.whl


# download release from lifemapper github
download_source () {
  echo "Starting download from $URL"
  wget "$URL/$FNAME" 
}

### main ###
download_source
