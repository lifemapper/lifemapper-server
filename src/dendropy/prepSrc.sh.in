#!/bin/bash

# Purpose: Fetch DendroPy source
DENDROPY_URL=https://files.pythonhosted.org/packages/f9/10/125c181b1d97ffc4661a60ec897cfe058dc46cb53900d807819464c3510f
DENDROPY_PKG=@ARCHIVENAME@-@VERSION@.@TARBALL_POSTFIX@

get_source_files () {
  echo "Fetch DendroPy source code"
  wget "$DENDROPY_URL/$DENDROPY_PKG"
}

### main ###
get_source_files
