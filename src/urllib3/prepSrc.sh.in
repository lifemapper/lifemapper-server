#!/bin/bash

# Purpose: Fetch @ARCHIVENAME@ wheel
DL_URL=https://files.pythonhosted.org/packages/9f/f0/a391d1463ebb1b233795cabfc0ef38d3db4442339de68f847026199e69d7
DL_WHL=@ARCHIVENAME@-@VERSION@-py2.py3-none-any.whl

get_wheel () {
  echo "Fetch @ARCHIVENAME@ wheel file"
  wget "$DL_URL/$DL_WHL"
}

### main ###
get_wheel
